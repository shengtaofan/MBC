  "browserslist": [
    ">= 1%",
    "not dead",
    "Chrome >= 88",
    "Firefox >= 78",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "axios": "^1.4.0",
    "bs4": "npm:bootstrap@^4.6.2",
    "bs5": "npm:bootstrap@^5.3.0",
    "lodash-es": "^4.17.21"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "standard"
    ],
    "rules": {
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-new": 0
    },
    "globals": {
      "$": "readonly",
      "ENV": "readonly"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "helpers": true,
          "corejs": false,
          "regenerator": true,
          "useESModules": false,
          "absoluteRuntime": false
        }
      ]
    ]
  }
