    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.2",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "puglint-webpack-plugin": "^1.0.3",
